{
  "normalization_map": {
    "~": "-",
    "～": "-",
    "—": "-",
    " ": "",
    "℃": "摄氏度",
    "°C": "摄氏度",
    "度": "摄氏度",
    "到": "-",
    "至": "-",
    "VDC": "V",
    "Vdc": "V",
    "VAC": "V",
    "Vac": "V",
    "mA DC": "mA",
    "mADC": "mA",
    "PPM": "ppm",
    "ppm": "ppm",
    "%RH": "%rh",
    "RH": "%rh",
    "相对湿度": "%rh",
    "Pa": "pa",
    "帕": "pa",
    "米/秒": "m/s",
    "米每秒": "m/s",
    "公斤": "kg",
    "千克": "kg",
    "毫米": "mm",
    "厘米": "cm",
    "米": "m",
    "DN": "dn",
    "PN": "pn",
    "IP": "ip",
    "Nm": "nm",
    "牛米": "nm",
    "扭矩": "nm"
  },
  "feature_split_chars": [",", ";", "，", "；", "：", ":", "/", "、", "|", "\\"],
  "ignore_keywords": [
    "施工要求",
    "验收",
    "图纸",
    "规范",
    "清单",
    "调试",
    "安装",
    "含税",
    "不含税",
    "备注",
    "说明",
    "要求",
    "标准",
    "国标",
    "进口",
    "国产",
    "品牌",
    "厂家",
    "供货",
    "交货",
    "质保"
  ],
  "global_config": {
    "default_match_threshold": 2,
    "unify_lowercase": true,
    "remove_whitespace": true,
    "fullwidth_to_halfwidth": true
  },
  "ui_config": {
    "max_file_size_mb": 10,
    "supported_formats": ["xls", "xlsm", "xlsx"],
    "default_export_format": "xlsx"
  },
  "performance_config": {
    "parse_timeout_seconds": 5,
    "match_timeout_seconds": 10,
    "max_rows_per_file": 10000
  }
}
