      "enabled": true,
      "patterns": [
        {
          "description": "提取精度、温度、湿度、量程中的数值（归一化后的格式）",
          "name": "精度规格",
          "pattern": "±(\\d+)%?@(\\d+)c\\.?(\\d+)%?rh\\((\\d+)[-~](\\d+)"
        }
      ]
    },
    "enabled": true,
    "feature_quality_scoring": {
      "enabled": true,
      "min_quality_score": 50,
      "scoring_rules": {
        "appropriate_length": 5,
        "has_number": 10,
        "has_unit": 10,
        "in_device_keywords": 15,
        "is_common_word": -20,
        "is_metadata_label": -30,
        "is_pure_number": -15,
        "is_pure_punctuation": -30,
        "is_technical_term": 20,
        "too_short": -20
      }
    },
    "metadata_label_patterns": [
      "\\d+\\.?名称[:：]",
      "\\d+\\.?规格[:：]",
