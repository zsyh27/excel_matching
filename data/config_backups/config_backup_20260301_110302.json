    "text_cleaning": {
      "enabled": true,
      "noise_section_patterns": [
        {
          "description": "删除按照图纸规范的描述",
          "pattern": "按照.*?(?:图纸|规范|清单).*?(?:要求|配置)"
        },
        {
          "description": "删除验收相关描述",
          "pattern": "(?:达到|满足|通过).*?验收"
        },
        {
          "description": "删除采购运输相关描述",
          "pattern": "(?:采购|运输|保管|转运).*?(?:施工|验收)"
        }
      ],
      "truncate_delimiters": [
        {
          "description": "在施工要求处截断",
          "pattern": "\\d+\\.?施工要求[:：]"
        },
        {
          "description": "在数字序列处截断",
          "pattern": ",个,\\d+,\\d+,"
        },
        {
          "description": "在采购说明处截断",
          "pattern": "含该项施工内容"
        }
      ]
    }
  },
